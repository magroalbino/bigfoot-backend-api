{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "redemptions.js",
      "use": "@vercel/edge"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/redemptions",
      "dest": "redemptions.js"
    }
  ]
}
